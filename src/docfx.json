{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/**.yml",
          "**/**.md"
        ],
        "src": "docs",
        "dest": ".",
        "exclude": [
          "readme.md",
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "readme.md",
          "Build.cmd",
          "Test.cmd",
          "assets/**",
          "images/**",
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif",
          "**/*.ppt",
          "**/*.pptx",
          "**/*.pdf",
          "logo.svg",
          "logo-light.svg",
          "tutorials_and_samples/**.png",
          "blog/media/**.png",
          "blog/media/**.jpg",
          "google8df5bb4c968b406e.html"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../_site",
    "template": [
      "default",
      "template"
    ],
    "noLangKeyword": false,
    "globalMetadata": {
      "_appTitle": "Microsoft Orleans Documentation",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/dotnet/orleans-docs",
        "branch": "main"
      },
	    "_gitUrlPattern": "github",
      "author": ""
    },
    "sitemap": {
      "baseUrl": "https://dotnet.github.io/orleans"
    }
  }
}
